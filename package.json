{
  "name": "if-libjs",
  "version": "1.0.0",
  "description": "Lightweight k-line chart built with html5 canvas",
  "main": "index.js",
  "types": "./types/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "docs/",
    "index.js",
    "types/",
    "dist/",
    "licenses/",
    "NOTICE"
  ],
  "repository": {
